{
  "name": "jarvis-ultra-instinct",
  "version": "2.0.0",
  "description": "BUREAU ULTRA INSTINCT - Dashboard IA complet avec génération de code",
  "main": "out/electron.js",
  "homepage": "./",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "node jarvis-cli-main.js",
    "dashboard": "node jarvis-cli-main.js dashboard",
    "electron": "cross-env NODE_ENV=development concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "build:electron": "node build-electron.js",
    "dist": "npm run build:electron && electron-builder",
    "dist:win": "npm run build:electron && electron-builder --win",
    "dist:mac": "npm run build:electron && electron-builder --mac",
    "dist:linux": "npm run build:electron && electron-builder --linux"
  },
  "keywords": [
    "ai",
    "dashboard",
    "code-generation",
    "react",
    "nextjs",
    "ultra-instinct",
    "jarvis",
    "groq"
  ],
  "author": "Guima",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@babel/parser": "^7.28.4",
    "@codesandbox/sandpack-react": "^2.20.0",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.16",
    "axios": "^1.4.0",
    "chalk": "^5.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^9.4.1",
    "figlet": "^1.8.2",
    "framer-motion": "^12.23.12",
    "groq-sdk": "^0.32.0",
    "inquirer": "^12.9.4",
    "lucide-react": "^0.263.0",
    "next": "^14.0.0",
    "node-fetch": "^3.3.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-force-graph-2d": "^1.25.0",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.0.0",
    "wait-on": "^8.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "build": {
    "appId": "com.guima.jarvis-ultra-instinct",
    "productName": "JARVIS Ultra Instinct",
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**/*",
      "electron.js",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.ico",
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "assets/icon.ico",
      "uninstallerIcon": "assets/icon.ico",
      "installerHeaderIcon": "assets/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "JARVIS Ultra Instinct"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.png"
    }
  }
}
